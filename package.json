{
  "name": "mttk-bi",
  "version": "0.0.8",
  "main": "./dist/mttk-bi.umd.js",
  "module": "./dist/mttk-bi.mjs",
  "exports": {
    ".": {
      "import": "./dist//mttk-bi.mjs",
      "require": "./dist//mttk-bi.umd.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "style": "dist/index.css",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build ",
    "build-test": "vite build -c vite.config.test.ts",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "analyze": "source-map-explorer 'dist/*.mjs'"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "echarts": "^5.4.2",
    "echarts-simple-transform": "^1.0.0",
    "echarts-stat": "^1.2.0",
    "echarts-wordcloud": "^2.1.0",
    "mttk-lowcode-engine": "^0.3.8",
    "vue": "^3.4.21", 
    "vue-echarts": "^6.6.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.71.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
